private void aaa() throws Exception{
    rs = con.excuteQuery("select * from hodan");
    DefaultComboBoxModel a = new DefaultComboBoxModel();
    while (rs.next()){
        a.addElement(rs.getString(2));
    }
    jList1.setModel(a);
    rs.close();
}
    private void loadList(String dk) throws Exception {
        String sql = null;
        if (jComboBox3.getSelectedIndex() == 0) {
            switch (jComboBox1.getSelectedIndex()) {
                case 4:
                    sql = ("SELECT * FROM hodan where Socnnn like '%" + dk + "%'");
                    break;
                case 1:
                    sql = ("SELECT * FROM hodan where mahd like '%" + dk + "%'");
                    break;
                case 2:
                    sql = ("SELECT * FROM hodan where tenhd like '%" + dk + "%'");
                    break;
                case 3:
                    sql = ("SELECT * FROM hodan where ngaysinh like '%" + dk + "%'");
                    break;
                default:
                    sql = "SELECT * FROM Noidung WHERE (Socnnn LIKE '%" + dk + "%') or (mahd LIKE '%" + dk + "%')or (tenhd LIKE '%" + dk + "%') or (ngaysinh LIKE '%" + dk + "%')";

            }
            rs = con.excuteQuery(sql);
            DefaultListModel model = new DefaultListModel();
            while (rs.next()) {
                model.addElement(rs.getString(1));
            }
            jList1.setModel(model);
            rs.close();
        }
        else {
            JOptionPane.showMessageDialog(null, sql);
        }
    }

