    }//GEN-LAST:event_jButton6ActionPerformed

    private void jList2ValueChanged(javax.swing.event.ListSelectionEvent evt) {//GEN-FIRST:event_jList2ValueChanged
        String sql = null;
        sql = "SELECT * FROM nguoidan where (hoten = '" + jList2.getSelectedValue().toString() + "') and (mahd = '" + jTextField1.getText() + "')";
        try {
            rs = con.excuteQuery(sql);
            rs.next();
            txt();
            rs.close();
        } catch (Exception ex) {
            Logger.getLogger(FTachHo.class.getName()).log(Level.SEVERE, null, ex);
        }
    }//GEN-LAST:event_jList2ValueChanged
    private void loadList(String dk) throws Exception {
        String sql = null;
        sql = "SELECT * FROM nguoidan where MaHD = '" + dk + "'";
        rs = con.excuteQuery(sql);
        DefaultListModel a = new DefaultListModel();
        while (rs.next()) {
            a.addElement(rs.getString(2));
        }
        jList2.setModel(a);
        rs.close();
        sql = "SELECT * FROM nguoidan where (MaHD = '" + dk + "') and (quanhe = 'Chủ Hộ')";
        rs = con.excuteQuery(sql);
        if (rs.next()) {
            tfchht.setText(rs.getString(2));
        }
        rs.close();
          
        sql = "SELECT * FROM nguoidan where (MaHD = '" + jTextField1.getText() + "') and (quanhe <> 'Chủ Hộ')";
        rs = con.excuteQuery(sql);
        DefaultListModel b = new DefaultListModel();
        while(rs.next()){
            b.addElement(rs.getString(2));
        }
        jList3.setModel(b);
        rs.close();
    }

