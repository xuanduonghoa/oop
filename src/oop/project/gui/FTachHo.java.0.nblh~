    private void jTextField1KeyReleased(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_jTextField1KeyReleased
        try {
            lList(jTextField1.getText());
        } catch (Exception ex) {
            Logger.getLogger(FQuanLyCB.class.getName()).log(Level.SEVERE, null, ex);
        }
    }//GEN-LAST:event_jTextField1KeyReleased

    private void jList1ValueChanged(javax.swing.event.ListSelectionEvent evt) {//GEN-FIRST:event_jList1ValueChanged
        String sql = null, dk = "Chủ Hộ", mahd = "";
        try {

            sql = ("SELECT * FROM hokhau where tenchuho = '" + jList1.getSelectedValue().toString() + "'");
            rs = con.excuteQuery(sql);
            rs.next();
            mahd = rs.getString(1);
            rs.close();

            sql = ("SELECT * FROM nguoidan where mahd = '" + mahd + "'");
            rs = con.excuteQuery(sql);
            DefaultListModel a = new DefaultListModel();
            while (rs.next()) {
                a.addElement(rs.getString(2));
            }
            jList2.setModel(a);
            setModel(a);
            // txt();
            rs.close();



            sql = "SELECT * FROM nguoidan WHERE (mahd = '" + mahd + "') and (quanhe = 'Chủ Hộ')";
            rs = con.excuteQuery(sql);
            rs.next();
            tfchht.setText(rs.getString(2));
            rs.close();


        } catch (Exception e) {
        }
    }//GEN-LAST:event_jList1ValueChanged

    private void jList2ValueChanged(javax.swing.event.ListSelectionEvent evt) {//GEN-FIRST:event_jList2ValueChanged
        DefaultListModel a = new DefaultListModel();       
        a.addElement(jList2.getSelectedValue().toString());
        jList4.setModel(a);
        setModel4(a);
//        model = new DefaultListModel();
//        model = getModel();
//        model.remove(jList2.getSelectedIndex());
        try {
            show(jList2.getSelectedValue().toString());
        } catch (Exception ex) {
            Logger.getLogger(FTachHo.class.getName()).log(Level.SEVERE, null, ex);

        }
    }//GEN-LAST:event_jList2ValueChanged

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton4ActionPerformed
        DefaultListModel a = new DefaultListModel();
        a= getModel4();
        a.addElement(jList3.getSelectedValue().toString());
        jList4.setModel(a);
        DefaultListModel b = new DefaultListModel();
        b  = getModel();//demo
        b.remove(jList3.getSelectedIndex());
        jList3.setModel(b);
    }//GEN-LAST:event_jButton4ActionPerformed

